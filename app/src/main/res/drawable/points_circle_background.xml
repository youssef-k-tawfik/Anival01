<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="0"
                android:endColor="#34b98b"
                android:startColor="#122d46"
                android:type="linear" />
        </shape>
    </item>
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="oval">
            <gradient
                android:endColor="#122d46"
                android:startColor="#34b98b"
                android:type="linear" />
        </shape>
    </item>
</layer-list>
